{
  "headers": [
    {
      "source": "/assets/(.*)\\.zip",
      "headers": [
        {
          "key": "Content-Disposition",
          "value": "attachment"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/apple-review/route25-screenshots-iphone.zip",
      "destination": "/assets/Route25-App-Store-Screenshots-iPhone.zip"
    },
    {
      "source": "/api/__proxy/:path*",
      "destination": "https://palettetown-backend.vercel.app/:path*"
    },
    {
      "source": "/api/proxy/:path*",
      "destination": "https://palettetown-backend.vercel.app/api/proxy/:path*"
    },
    {
      "source": "/login",
      "destination": "https://palettetown-backend.vercel.app/login"
    },
    {
      "source": "/login/:path*",
      "destination": "https://palettetown-backend.vercel.app/login/:path*"
    },
    {
      "source": "/web",
      "destination": "https://palettetown-backend.vercel.app/web"
    },
    {
      "source": "/web/:path*",
      "destination": "https://palettetown-backend.vercel.app/web/:path*"
    },
    {
      "source": "/api/web",
      "destination": "https://palettetown-backend.vercel.app/api/web"
    },
    {
      "source": "/api/web/:path*",
      "destination": "https://palettetown-backend.vercel.app/api/web/:path*"
    }
  ],
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ]
}
